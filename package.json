{
  "name": "ghostban",
  "version": "3.0.0-alpha.31",
  "description": "A Goban library",
  "main": "build/index.js",
  "browser": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "git@github.com:ghost-go/ghostban.git",
  "author": "BAI TIANLIANG",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c && tsc --emitDeclarationOnly",
    "watch": "rollup -w -c",
    "lint": "gts lint",
    "test": "jest",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "start": "npx http-server ./build",
    "watch-pub-up": "watchman-make -p 'build/index.min.js' --run 'sh publish-dev.sh'"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.5.2",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.3.2",
    "@types/string.prototype.matchall": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "babel-plugin-import": "^1.13.6",
    "eslint": "^8.43.0",
    "gts": "^3.1.1",
    "jest": "^29.5.0",
    "rollup": "^3.25.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.5"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "lodash-es": "^4.17.21",
    "string.prototype.matchall": "^4.0.8",
    "tree-model": "^1.0.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ]
  }
}
