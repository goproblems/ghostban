{
  "name": "ghostban",
  "version": "3.0.0-alpha.29",
  "description": "A Goban library",
  "main": "build/index.js",
  "browser": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "git@github.com:ghost-go/ghostban.git",
  "author": "BAI TIANLIANG",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c && tsc --emitDeclarationOnly",
    "watch": "rollup -w -c",
    "lint": "gts lint",
    "test": "jest",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "start": "npx http-server ./build",
    "watch-pub-up": "watchman-make -p 'build/index.min.js' --run 'sh publish-dev.sh'"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.176",
    "@types/node": "^14.11.2",
    "@types/string.prototype.matchall": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^8.1.0",
    "gts": "^3.1.0",
    "jest": "^29.3.1",
    "rollup": "^2.58.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "lodash": "^4.17.21",
    "mathjs": "^11.5.0",
    "string.prototype.matchall": "^4.0.8",
    "tree-model": "^1.0.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ]
  }
}
